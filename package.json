{
  "name": "rk-squared",
  "productName": "RK Squared",
  "version": "1.1.1",
  "description": "RK Squared tracker for Final Fantasy Record Keeper",
  "main": "./app/main.prod.js",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build:watch": "tsc -w",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.js --colors",
    "build-main": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.main.prod.js --progress --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config configs/webpack.config.renderer.prod.js --progress --colors",
    "dev": "cross-env START_HOT=1 node ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start-renderer-dev",
    "predev": "yarn copy-json",
    "package": "yarn build && build --publish never",
    "package:all": "yarn build && build -mw",
    "package:linux": "yarn build && build --linux",
    "package:win": "yarn build && build --win --x64",
    "lint": "tslint --project .",
    "lint:watch": "onchange 'scripts/*.ts' 'app/**/*.ts' 'app/**/*.tsx' -- yarn lint",
    "start": "cross-env NODE_ENV=production electron ./app/main.prod.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron ./dist/app/main.js",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.dev.js",
    "test": "jest",
    "test:all": "yarn test && yarn build && yarn test-e2e",
    "test:watch": "jest --watch",
    "copy-json": "copyfiles --flat \"./app/data/enlir/*.json\" ./dist/app/data/enlir",
    "get-enlir": "ts-node scripts/download-enlir.ts -o app/data/enlir"
  },
  "jest": {
    "testURL": "http://localhost/",
    "roots": [
      "<rootDir>/app"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/app/**/*.{ts,tsx}",
      "!**/__tests__/**"
    ],
    "coverageReporters": [
      "html"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint"
    ]
  },
  "build": {
    "productName": "RK Squared",
    "appId": "com.joshkel.rksquared",
    "files": [
      "app/dist/",
      "app/app.html",
      "app/main.prod.js",
      "app/main.prod.js.map",
      "package.json",
      "!**/node_modules/**/*",
      "node_modules/**/*.css"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rk-squared/rk-squared.git"
  },
  "author": {
    "name": "Josh Kelley",
    "url": "https://github.com/joshkel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rk-squared/rk-squared/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/rk-squared/rk-squared#readme",
